#include<iostream>
#include<cmath>
using namespace std;
int main(){
	int n;
	cin>>n;
	int arr[n];
	for(int i =0;i<n;i++){
		cin>>arr[i];
	}
	int maxxx = arr[0];
	int minn = arr[0];
	for(int i =0;i<n;i++){
	maxxx = max(maxxx,arr[i]);
	minn = min(minn, arr[i]);
    } 
	cout<<"maximum is "<<maxxx<<" "<<"minimum is "<<minn<<endl;  
}
