#include<iostream>
#include<cmath>
using namespace std;
int main(){
	int n; 
	cin>>n;
	int arr[n],temp[n];
	for(int i =0;i<n;i++){
		cin>>arr[i];
	}
	
	for(int i =0;i<n;i++){
		temp[i+1] = arr[i];
	}
	temp[0] = arr[n-1];
	 int i = 0;	
	while(n--){
	   arr[i] = temp[i];
	   i++;
	}

	for(int i =0;i<n;i++){
		cout<<arr[i]<<" ";
	}
}
